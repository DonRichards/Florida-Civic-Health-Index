extends ../layout

block content
	// Attended Public Meetings
	.hero-wrapper
		.videoBG_wrapper
			.container
				.row
					Meetings.articleText
						h1
							span.nowrap Attended Public Meeting
		.videoBG
			video(poster='/images/meetings.jpg')
	.ALtable
		table.smallBarTable
			tbody
				tr
					td(colspan='2', align='left', valign='top', width='225', style='font-weight: bold; padding-top: 3px; padding-left: 13px;') 2012
					td(colspan='2', width='217', align='left', valign='top', style='font-weight: bold; padding-top: 3px; padding-left: 13px;') 
						span.red Attended Public Meeting 
						span.right
							a(href="http://104.130.206.32/discussion/forum/general/", target="_blank")  
								img(src='/uploads/2014/01/discuss.png').discussIcon
				tr
					td(width='40', align='left', valign='top').tableState
						img(src='/images/states/Orange_Florida_icon.png', alt='', title='').tableStateIcon
						img(src='/images/state_icons/Flag-of-United-States.png', alt='', title='').tableStateIcon
					td(width='auto', align='left', valign='top').tableNames

						| Florida
						br
						| National Average
						br
					td(width='155', align='left', valign='top').tableBar#barMeeting
					td(width='35', align='left', valign='top').tableNames
						span.red 7.1%
						br
						span.red 9.0%
						br
	script(src='../javascripts/d3.v3.js')
	script(src='../javascripts/volunteering.js')