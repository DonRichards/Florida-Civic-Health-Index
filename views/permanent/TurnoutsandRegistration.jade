extends ../layout

block content
	.hero-wrapper
		.videoBG_wrapper
			.container
				.row
					article.articleText
						h1
							span.nowrap Voter Turnout
		.videoBG
			video(poster='/images/guy_crowd.jpg').blur
	.ALtable
		table.smallBarTable
			tbody
				tr
					td(colspan='2', align='left', valign='top', width='225', style='font-weight: bold; padding-top: 3px; padding-left: 13px;') 2012
					td(colspan='2', width='217', align='left', valign='top', style='font-weight: bold; padding-top: 3px; padding-left: 13px;') 
						span.red Voter Turnout
				tr
					td(width='40', align='left', valign='top').tableState
						img(src='/images/states/Orange_Florida_icon.png', alt='', title='').tableStateIcon
						img(src='/images/state_icons/Flag-of-United-States.png', alt='', title='').tableStateIcon
					td(width='auto', align='left', valign='top').tableNames
						| Florida
						br
						| National Average
						br
					td(width='155', align='left', valign='top').tableBar#barTurnout
					td(width='35', align='left', valign='top').tableNames
						span.red 60.8%
						br
						span.red 61.8%
						br
	.hero-wrapper
		.videoBG_wrapper
			.container
				.row
					article2.articleText
						h1
							span.nowrap Registered to Vote
		.videoBG
			video(poster='/images/form.jpg').blur
	.ALtable
		table.smallBarTable
			tbody
				tr
					td(colspan='2', align='left', valign='top', width='225', style='font-weight: bold; padding-top: 3px; padding-left: 13px;') 2012
					td(colspan='2', width='217', align='left', valign='top', style='font-weight: bold; padding-top: 3px; padding-left: 13px;') 
						span.red Registerd to Vote
				tr
					td(width='40', align='left', valign='top').tableState
						img(src='/images/states/Orange_Florida_icon.png', alt='', title='').tableStateIcon
						img(src='/images/state_icons/Flag-of-United-States.png', alt='', title='').tableStateIcon
					td(width='auto', align='left', valign='top').tableNames
						| Florida
						br
						| National Average
						br
					td(width='155', align='left', valign='top').tableBar#barRegistration
					td(width='35', align='left', valign='top').tableNames
						span.red 60.3%
						br
						span.red 71.2%
						br
